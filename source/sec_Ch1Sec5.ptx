<p id="fs-id1170572168770"><m><m:msup>4−2</m:msup>=<!-- Edit FRAC!--> \frac{116}</m></p>
</statement>
<solution id="fs-id1170572168791">
<p id="fs-id1170572168793"><m>\log_{4}(<!-- Edit FRAC!--> \frac{116})=−2</m></p>
</solution>


<!-- Move title outside statement--> <statement id="fs-id1170572168825">
<p id="fs-id1170572168827"><m>10^2=100</m></p>
</statement>


<!-- Move title outside statement--> <statement id="fs-id1170572168862">
<p id="fs-id1170572168865"><m>9^0=1</m></p>
</statement>
<solution id="fs-id1170572168880">
<p id="fs-id1170572168882"><m>\log_{9}1=0</m></p>
</solution>


<!-- Move title outside statement--> <statement id="fs-id1170572218181">
<p id="fs-id1170572218184"><m><m:msup>(\frac{1}{3})3</m:msup>=<!-- Edit FRAC!--> \frac{127}</m></p>
</statement>


<!-- Move title outside statement--> <statement id="fs-id1170572218253">
<p id="fs-id1170572218255"><m><m:mroot>643</m:mroot>=4</m></p>
</statement>
<solution id="fs-id1170572218271">
<p id="fs-id1170572218273"><m>\log_{64}4=\frac{1}{3}</m></p>
</solution>


<!-- Move title outside statement--> <statement id="fs-id1170572218299">
<p id="fs-id1170572218301"><m>e^x=y</m></p>
</statement>


<!-- Move title outside statement--> <statement id="fs-id1170572218335">
<p id="fs-id1170572218337"><m>9^y=150</m></p>
</statement>
<solution id="fs-id1170572218352">
<p id="fs-id1170572218354"><m>\log_{9}150=y</m></p>
</solution>


<!-- Move title outside statement--> <statement id="fs-id1170572218376">
