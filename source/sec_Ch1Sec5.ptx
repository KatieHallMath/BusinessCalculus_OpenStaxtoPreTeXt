</statement>


<!-- Move title outside statement--> <statement id="fs-id1170572543673">
<p id="fs-id1170572543675"><m>\log_{5}x=−2</m></p>
</statement>
<solution id="fs-id1170572543693">
<p id="fs-id1170572543696"><m>x=<!-- Edit FRAC!--> \frac{125}</m></p>
</solution>


<!-- Move title outside statement--> <statement id="fs-id1170572543715">
<p id="fs-id1170572543717"><m>\log_{4}(x+5)=0</m></p>
</statement>


<!-- Move title outside statement--> <statement id="fs-id1170572543763">
