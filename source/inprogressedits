<entry align="center"><m>0</m></entry>
<entry align="center"><m>0.5</m></entry>
<entry align="center"><m>1</m></entry>
<entry align="center"><m>1.5</m></entry>
<entry align="center"><m>2</m></entry>
<entry align="center"><m>2.5</m></entry>
