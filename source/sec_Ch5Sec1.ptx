<md>
  <mrow>\displaystyle{  \sum_{i=1}^{n} ca_i}\amp =ca_1+ca_2+ca_3+ \cdots +ca_n</mrow>
  <mrow>\amp=c(a_1+a_2+a_3+ \cdots +a_n)</mrow>
